doctype html
html
  head
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(charset='UTF-8')
    meta(name='viewport', content='user-scalable=yes, width=device-width, initial-scale=1, maximum-scale=5')
    meta(name='theme-color', content='#333333')
    meta(name='msapplication-TileColor', content='#333333')
    meta(name='msapplication-TileImage', content='/favicons/ms-icon-144x144.png')

    title= pageMeta.title + ' | ' + config.title

    //- SEO / OpenGraph
    meta(name='description', content=pageMeta.description)
    meta(property='og:title', content=pageMeta.title)
    meta(property='og:type', content='website')
    meta(property='og:description', content=pageMeta.description)
    meta(property='og:image', content=pageMeta.image)
    meta(property='og:url', content=pageMeta.url)
    meta(property='og:site_name', content=config.title)

    //- Favicon
    each favsize in [57, 60, 72, 76, 114, 120, 144, 152, 180]
      link(rel='apple-touch-icon', sizes=favsize + 'x' + favsize, href='/favicons/apple-icon-' + favsize + 'x' + favsize + '.png')
    link(rel='icon', type='image/png', sizes='192x192', href='/favicons/android-icon-192x192.png')
    each favsize in [32, 96, 16]
      link(rel='icon', type='image/png', sizes=favsize + 'x' + favsize, href='/favicons/favicon-' + favsize + 'x' + favsize + '.png')
    link(rel='manifest', href='/manifest.json')

    //- Icon Set
    if config.theming.iconset === 'fa'
      link(
        type='text/css'
        rel='stylesheet'
        href='https://use.fontawesome.com/releases/v5.10.0/css/all.css'
        )
    else if config.theming.iconset === 'fa4'
      link(
        type='text/css'
        rel='stylesheet'
        href='https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css'
        )

    //- CSS
    
      
    link(
      type='text/css'
      rel='stylesheet'
      href='/css/legacy.35e769d2aa1b0034b876.css'
      integrity='sha256-9llMGTpmzU76QOAZxa2dBxJE5neguiBePempiJQd3kA= sha512-k5ShEOeUPoFAXKDXe9KyO1KDw070tFs92egEcnweUMi8h8bcrFoKpsa9bZEL0j8s5Sb1UrZkr6DCCGEjlIeVsQ=='
      crossorigin='use-credentials'
    )
      
    

    script(
      crossorigin='anonymous'
      src='https://polyfill.io/v3/polyfill.min.js?features=EventSource'
    )

    //- JS
    
      
    script(
      type='text/javascript'
      src='/js/runtime.9bd61e09a8b881828543.js'
      integrity='sha256-cCKJV5sKXvmhhcBi1/IHDjaSYbdjL+WT62bHmQk/t/s= sha512-Wu6rRUvDtOkophjOYOX8FPxqOHSEmXzLh82liZiIsdSk48DMCCGWVthrhaTG8+B1vEfDgTryJO0Bb4FAYjXf9A=='
      crossorigin='use-credentials'
      )
      
    
      
    script(
      type='text/javascript'
      src='/js/legacy.35e769d2aa1b0034b876.js'
      integrity='sha256-xlgMHM9qmU2QRsBUgK4tRv4ZCJ3jxcCK2dTtq75pCtg= sha512-e5U9I1v7p5vmBRpdSUio21yoo61py3ft5wfvSACbjxWc7/oLhcnA/BeG4M0CdhWNj+pfDwKEubgdzRZSfUDzvQ=='
      crossorigin='use-credentials'
      )
      
    

    != analyticsCode.head

    block head

  body
    != analyticsCode.bodyStart
    block body
    != analyticsCode.bodyEnd
